> {\n    let [state, dispatch] = useReducer(Reducer, students);\n    \n    function getIndex(index) {\n        var newArray = [];\n        for (var i=0; i<state.length; i++) {\n            if ( i === index )\n                continue;\n            newArray.push(state[i])\n        }\n        /*console.log(newArray)*/\n        /*This for loop will skip the particular index and append all elements of state in newArray */\n        dispatch(\n            {\n                type: 'REMOVE',\n                payload: newArray\n            }\n        )\n    }\n\n    return (\n        <Context.Provider value={ {\n            students: state,\n            getIndex\n        } }>\n        { children }\n        </Context.Provider>\n    );\n}","/home/asad/Cloud-Computing/BootCamp2020-old/BootCamp2020-Class06/payload-action-context-reducer-remove-list-by-id/src/components/ClassX-A.js",[],"/home/asad/Cloud-Computing/BootCamp2020-old/BootCamp2020-Class06/payload-action-context-reducer-remove-list-by-id/src/App.js",[],{"ruleId":"57","severity":1,"message":"58","line":1,"column":8,"nodeType":"59","messageId":"60","endLine":1,"endColumn":13},{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","replacedBy":"64"},{"ruleId":"57","severity":1,"message":"58","line":1,"column":8,"nodeType":"59","messageId":"60","endLine":1,"endColumn":13},"no-unused-vars","'react' is defined but never used.","Identifier","unusedVar","no-native-reassign",["65"],"no-negated-in-lhs",["66"],"no-global-assign","no-unsafe-negation"]